{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "coverageDirectory": "../tests/coverage",
  "setupFilesAfterEnv": ["./tests/jestSetupAfterEnv.ts"],
  "globalSetup": "<rootDir>/tests/jestGlobalSetup.ts",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
