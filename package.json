{
  "name": "nestjs-mesh",
  "version": "0.0.2",
  "description": "NestJS Mesh",
  "author": "Nguyen Van Vy",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:me824/Mesh_AI_Backend.git"
  },
  "bugs": {
    "url": "https://gitlab.com/me824/Mesh_AI_Backend/issues"
  },
  "homepage": "https://gitlab.com/me824/Mesh_AI_Backend#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"{src,test,e2e,integration}/**/*.ts\"",
    "format:src": "prettier --write src/**/*.ts",
    "format:test": "prettier --write test/**/*.ts",
    "format:e2e": "prettier --write e2e/**/*.ts",
    "format:integration": "prettier --write integration/**/*.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "eslint --ext .ts,.tsx \"{src,test,e2e,integration}/**/*.ts\" --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --ext .ts,.tsx \"{src,test,e2e,integration}/**/*.ts\" --fix --no-error-on-unmatched-pattern",
    "lint:test": "eslint --ext .ts,.tsx test/**/*.ts --no-error-on-unmatched-pattern",
    "lint:src": "eslint --ext .ts,.tsx src/**/*.ts --no-error-on-unmatched-pattern",
    "lint:e2e": "eslint --ext .ts,.tsx e2e/**/*.ts --no-error-on-unmatched-pattern",
    "lint:integration": "eslint --ext .ts,.tsx integration/**/*.ts --no-error-on-unmatched-pattern",
    "test": "jest --config test/jest.json --detectOpenHandles --passWithNoTests --forceExit --logHeapUsage -w 1",
    "test:unit": "jest --config test/jest.json --detectOpenHandles --passWithNoTests --forceExit --logHeapUsage -w 1",
    "test:e2e": "jest --config e2e/jest.json --detectOpenHandles --passWithNoTests --forceExit --logHeapUsage -w 1",
    "test:integration": "jest --config integration/jest.json --detectOpenHandles --passWithNoTests --forceExit --logHeapUsage -w 1",
    "postinstall": "husky install",
    "lint:staged": "npx lint-staged",
    "prestart:prod": "npm run build",
    "deadcode": "ts-prune --project tsconfig.json",
    "seed": "npx nestjs-command insert:device"
  },
  "dependencies": {
    "@faker-js/faker": "^6.3.1",
