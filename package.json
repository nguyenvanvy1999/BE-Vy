    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:all && npm run lint && npm run format"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format"
    ]
  },
  "engines": {
    "node": "17.1.0",
    "npm": "8.1.4"
  }
}
