    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.2",
    "dotenv": "^16.0.1",
    "exceljs": "^4.3.0",
    "express": "^4.18.1",
    "express-jwt": "^6.1.2",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.0.2",
    "lodash": "^4.17.21",
    "mongodb": "^4.6.0",
    "mongoose": "^6.3.3",
    "morgan": "^1.10.0",
