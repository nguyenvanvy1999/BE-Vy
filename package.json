    "nest-aws-sdk": "^2.1.0",
    "nest-winston": "^1.6.2",
    "nestjs-asyncapi": "^0.2.10",
    "nestjs-command": "^3.1.1",
    "nestjs-i18n": "^9.0.12",
    "nestjs-redoc": "^2.2.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "prom-client": "^14.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rotating-file-stream": "^3.0.3",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.4.0",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1",
    "winston-transport": "^4.5.0",
    "yargs": "^17.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-angular": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
